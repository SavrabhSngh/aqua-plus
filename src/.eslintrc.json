{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended",
        "react-app"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "no-console": "warn",
        "no-debugger": "error",
        "react-hooks/exhaustive-deps": "off"
    },
    "settings": {
        "react": {
            "pragma": "React",
            "version": "detect"
        }
    }
}